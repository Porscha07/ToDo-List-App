<!DOCTYPE html>
<html>
<head>
	<title>To-Do List App</title>

	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"> 
	<link rel="stylesheet" type="text/css" href="stylesheets/style.css">
</head>

<body>
	<div class="container-fluid">
		<div class="row">
			<div class="col-sm-12 text-center weekly-header">
				<h1>Weekly To Do List</h1>
			</div>
		</div>
		<!-- **************************Weekly Row*********************** -->
			<div class="row">
				<div class="col-sm-12 text center weekly-print">
					<hr>
						<h4>Weekly</h4>
				</div>
			</div>
			<div class="row">
				<div class="text-black col-sm-12 weekly-row">
					<div class="col-sm-2 monday">
						<h3>Monday</h3>
					</div>
							<div class="col-sm-2 tuesday">
								<h3>Tuesday</h3>
							</div>
									<div class="col-sm-2 wednesday">
										<h3>Wednesday</h3>
									</div>
										<div class="col-sm-2 thursday">
											<h3>Thursday</h3>
										</div>
											<div class="col-sm-2  friday">
												<h3>Friday</h3>
											</div>
			</div>
		<!-- *********************************Must-Do Row***************************** -->
												<div class="row col-sm-12">
													<div class="text-black col-sm-6 must-do">
														<h3>Must-Do</h3>
													</div>
														<div class="row col-sm-12">
															<div class="text-black col-sm-6 complete-list">
																		<h3>Complete-List</h3>
															</div>
														</div>
												</div>
													<div class="col-sm-12">
														<div class="text-black col-sm-3 calls">
															<h3>Calls to Make</h3>
														</div>
															<div class="text-black col-sm-3 emails">
																<h3> Emails to Write</h3>
															</div>
													</div>
																<div class="row col-sm-12">
																	<div class="text-back col-sm-6 get-done">
																		<h3>Just get it done</h3>
																	</div>
																</div>
																	<div class="text-back col-sm-6  notes">
																		<h3>Notes</h3>
																	</div>
													</div>
																
<!-- 																	<div class="row col-sm-12">
																			<div class="text-back col-sm-6 get-done">
																				<h3>Just get it done</h3>
																			</div>
																	</div>
																		<div class="row-col-sm-12">
																				<div class="text-back col-sm-6  notes">
																					<h3>Notes</h3>
																				</div>
																		</div>	 -->										
								
				</div>
			</div>
									

	</div>
</body>
</html>
